<h1>new user page</h1>

<%= form_for User.new do |f| %>
<%= f.label :email %>
<%= f.text_field :email %>
<%= f.label :password %>
<%= f.password_field :password %>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation%>
<%= f.label :type %>
<%= f.select(:type, @options)%>
<%= f.submit "Sign Up" %>
<% end %>
